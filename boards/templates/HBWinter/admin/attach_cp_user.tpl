<table width="80%" cellpadding="0" cellspacing="0" align="center" class="MainTable">  <tr>    <td class="BoardColumn" align="center">        <span class="BoardColumnLink">{L_CONTROL_PANEL_TITLE}</span>    </td>  </tr>  <tr>    <td class="BoardRowB" align="center">        <br /><span class="BoardRowBLink">{L_CONTROL_PANEL_EXPLAIN}</span><br /><br />    </td>  </tr></table><br /><form method="post" action="{S_MODE_ACTION}">  <table width="100%" cellspacing="2" cellpadding="2" border="0" align="center">	<tr> <!--	  <td align="left" nowrap="nowrap"><span class="gen"><a class="gen" href="{U_BACK}" />Back</a></span></td> -->	  <td class="BoardRowA" align="right" nowrap="nowrap"><span class="genmed">{L_VIEW}:&nbsp;{S_VIEW_SELECT}&nbsp;&nbsp;{L_SELECT_SORT_METHOD}:&nbsp;{S_MODE_SELECT}&nbsp;&nbsp;{L_ORDER}&nbsp;{S_ORDER_SELECT}&nbsp;&nbsp; 		<input type="submit" name="submit" value="{L_SUBMIT}" class="liteoption" />		</span>	  </td>	</tr>  </table>  <table width="100%" cellpadding="3" cellspacing="1" border="0" class="forumline">	<tr> 	  <th height="25" class="BoardRowA">#</th>	  <th class="BoardRowA">{L_USERNAME}</th>	  <th class="BoardRowA">{L_ATTACHMENTS}</th>	  <th class="BoardRowA">{L_TOTAL_SIZE}</th>	</tr>	<!-- BEGIN memberrow -->	<tr> 	  <td class="BoardRowB" align="center"><span class="gen">&nbsp;{memberrow.ROW_NUMBER}&nbsp;</span></td>	  <td class="BoardRowB" align="center"><span class="gen"><a href="{memberrow.U_VIEW_MEMBER}" class="gen">{memberrow.USERNAME}</a></span></td>	  <td class="BoardRowB" align="center" valign="middle">&nbsp;<b>{memberrow.TOTAL_ATTACHMENTS}</b>&nbsp;</td>	  <td class="BoardRowB" align="center">&nbsp;<b>{memberrow.TOTAL_SIZE}</b>&nbsp;</td>	</tr>	<!-- END memberrow -->	<tr> 	  <td class="catbottom" colspan="4" height="28">&nbsp;</td>	</tr>  </table>  <table width="100%" cellspacing="2" border="0" align="center" cellpadding="2">	<tr> 	  <td align="right" valign="top"></td>	</tr>  </table><table width="100%" cellspacing="0" cellpadding="0" border="0">  <tr> 	<td><span class="nav">{PAGE_NUMBER}</span></td>	<td align="right"><span class="nav">{PAGINATION}&nbsp;</span></td>  </tr></table></form><br /><table width="40%" cellpadding="0" cellspacing="0" align="center" class="MainTable">  <tr>    <td class="BoardRowA" align="center">        <span class="copyright">{ATTACH_VERSION}</span>    </td>  </tr></table>