<table width="80%" cellpadding="0" cellspacing="0" align="center" class="MainTable">  <tr>    <td class="BoardColumn" align="center">        <span class="BoardColumnLink">{L_EXTENSIONS_TITLE}</span>    </td>  </tr>  <tr>    <td class="BoardRowB" align="center">        <br /><span class="BoardRowBLink">{L_EXTENSIONS_EXPLAIN}</span><br /><br />    </td>  </tr></table><br />{ERROR_BOX}<form method="post" action="{S_ATTACH_ACTION}">  <table width="100%" align="center" cellpadding="4" cellspacing="1" border="0" class="forumline">	<tr> 	  <td class="BoardRowA" colspan="5" align="center" height="28"><span class="cattitle">{L_EXTENSIONS_TITLE}</span> 	  </td>	</tr>	<tr> 	  <th class="BoardRowA">&nbsp;{L_EXPLANATION}&nbsp;</th>	  <th class="BoardRowA">&nbsp;{L_EXTENSION}&nbsp;</th>	  <th class="BoardRowA">&nbsp;{L_EXTENSION_GROUP}&nbsp;</th>	  <th class="BoardRowA">&nbsp;{L_ADD_NEW}&nbsp;</th>	</tr>	<tr>	  <td class="BoardRowB" align="center" valign="middle"><input type="text" size="30" maxlength="100" name="add_extension_explain" class="post" value="{ADD_EXTENSION_EXPLAIN}" /></td>	  <td class="BoardRowB" align="center" valign="middle"><input type="text" size="20" maxlength="100" name="add_extension" class="post" value="{ADD_EXTENSION}" /></td>	  <td class="BoardRowB" align="center" valign="middle">{S_ADD_GROUP_SELECT}</td>	  <td class="BoardRowB" align="center" valign="middle"><input type="checkbox" name="add_extension_check" /></td>	</tr>	<tr align="right"> 	  <td class="catBottom" colspan="5" height="29"> {S_HIDDEN_FIELDS} <input type="submit" name="submit" class="liteoption" value="{L_SUBMIT}" /></td>    </tr>	<tr> 	  <th class="BoardRowA">&nbsp;{L_EXPLANATION}&nbsp;</th>	  <th class="BoardRowA">&nbsp;{L_EXTENSION}&nbsp;</th>	  <th class="BoardRowA">&nbsp;{L_EXTENSION_GROUP}&nbsp;</th>	  <th class="BoardRowA">&nbsp;{L_DELETE}&nbsp;</th>	</tr><!-- BEGIN extension_row -->	<tr> 	  <input type="hidden" name="extension_change_list[]" value="{extension_row.EXT_ID}" />	  <td class="BoardRowB" align="center" valign="middle"><input type="text" size="30" maxlength="100" name="extension_explain_list[]" class="post" value="{extension_row.EXTENSION_EXPLAIN}" /></td>	  <td class="BoardRowB" align="center" valign="middle"><b><span class="gen">{extension_row.EXTENSION}</span></b></td>	  <td class="BoardRowB" align="center" valign="middle">{extension_row.S_GROUP_SELECT}</td>	  <td class="BoardRowB" align="center" valign="middle"><input type="checkbox" name="extension_id_list[]" value="{extension_row.EXT_ID}" /></td>	</tr><!-- END extension_row -->	<tr align="right"> 	  <td class="catBottom" colspan="5" height="29"> 	  <input type="submit" name="{L_CANCEL}" class="liteoption" value="{L_CANCEL}" onClick="self.location.href='{S_CANCEL_ACTION}'" />	  <input type="submit" name="submit" class="liteoption" value="{L_SUBMIT}" /></td>	</tr></table></form><br /><table width="40%" cellpadding="0" cellspacing="0" align="center" class="MainTable">  <tr>    <td class="BoardRowA" align="center">        <span class="copyright">{ATTACH_VERSION}</span>    </td>  </tr></table><br clear="all" />